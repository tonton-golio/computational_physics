{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "github": {
    "enabled": true,
    "silent": false
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}
